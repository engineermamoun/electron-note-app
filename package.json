{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/images/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . todo-list-app --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/images/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . todo-list-app --overwrite --asar --platform=linux --arch=x64 --icon=aassets/images/icon.png --prune=true --out=release-builds"
  },
  "author": "Med Mahmoud Ahmedou",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "electron": "^33.0.2",
    "jquery": "^3.7.1"
  },
  "dependencies": {
    "electron-packager": "^17.1.2",
    "jsstore": "^4.8.1"
  }
}
